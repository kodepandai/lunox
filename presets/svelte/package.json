    "svelte-preprocess": "^4.9.8",
    "ts-jest": "^27.1.4",
    "tslib": "^2.3.1",
    "typescript": "^4.7.3",
    "unocss": "^0.22.0",
    "vite": "^4.1.4"
  },
  "type": "module"
