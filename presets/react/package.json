{
  "name": "lunox-react",
  "version": "2.0.0-next",
  "private": true,
  "description": "Laravel-Flavoured NodeJs Framework",
  "main": "index.js",
  "scripts": {
    "artisan": "npm run build && node dist/artisan.js",
    "artisan:prod": "NODE_ENV=production node dist/artisan.js",
    "build": "lunox prod --with-view",
    "dev": "lunox dev",
    "serve": "lunox serve",
    "lint": "tsc --noEmit && prettier . --check && eslint . --fix",
    "prettier": "prettier . --write",
    "format": "npm run prettier && npm run lint",
    "test": "APP_ENV=testing vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodepandai/lunox.git"
  },
  "keywords": [
    "laravel",
    "nodejs",
    "polka",
    "react",
    "mvc"
  ],
  "author": "",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kodepandai/lunox/issues"
  },
  "homepage": "https://github.com/kodepandai/lunox#readme",
  "dependencies": {
    "@kodepandai/flydrive": "3.0.1",
    "@kodepandai/flydrive-s3": "3.0.1",
    "@lunoxjs/auth": "2.0.0-beta.1",
    "@lunoxjs/core": "2.0.0-beta.1",
    "@lunoxjs/event": "2.0.0-beta.1",
    "@lunoxjs/filesystem": "2.0.0-beta.1",
    "@lunoxjs/mail": "2.0.0-beta.1",
    "@lunoxjs/session": "2.0.0-beta.1",
    "@lunoxjs/typeorm": "2.0.0-beta.1",
    "@lunoxjs/validation": "2.0.0-beta.1",
    "@lunoxjs/view": "2.0.0-beta.1",
    "@lunoxjs/view-plugin-react": "2.0.0-beta.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "mysql2": "^3.6.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "session-file-store": "^1.5.0",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@iconify-json/ant-design": "^1.1.12",
    "@iconify-json/file-icons": "^1.1.7",
    "@lunoxjs/test": "2.0.0-beta.1",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.39",
    "@types/react-helmet": "^6.1.9",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "@unocss/reset": "^0.57.7",
    "@vitejs/plugin-react": "^4.2.0",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "supertest": "^6.3.3",
    "tslib": "^2.6.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "unocss": "^0.57.7",
    "vite": "^5.2.7",
    "vitest": "^0.34.6"
  },
  "type": "module"
