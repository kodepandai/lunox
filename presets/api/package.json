  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kodepandai/lunox/issues"
  },
  "homepage": "https://github.com/kodepandai/lunox#readme",
  "dependencies": {
    "@kodepandai/flydrive": "3.0.0-next",
    "@kodepandai/flydrive-s3": "3.0.0-next",
    "@lunoxjs/auth": "2.0.0-next.33",
    "@lunoxjs/core": "2.0.0-next.33",
    "@lunoxjs/event": "2.0.0-next.33",
    "@lunoxjs/filesystem": "2.0.0-next.33",
    "@lunoxjs/mail": "2.0.0-next.33",
