    "@types/node": "^16.11.7",
    "@typescript-eslint/eslint-plugin": "^5.3.1",
    "@typescript-eslint/parser": "^5.3.1",
    "eslint": "^8.2.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.16",
    "prettier": "^2.7.0",
    "rollup": "^3.19.1",
    "rollup-plugin-delete": "^2.0.0",
