{
  "name": "@lunoxjs/core",
  "version": "2.0.0-beta.3.8.0",
  "description": "Laravel-Flavoured NodeJs framework",
  "bin": {
    "lunox": "./bin/lunox.cjs"
  },
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./build": "./dist/build/index.js",
    "./helpers": "./dist/helpers.js",
    "./contracts": "./dist/Contracts/index.d.ts",
    "./facades": "./dist/Support/Facades/index.js",
    "./console": "./dist/Console/index.js"
  },
  "files": [
    "dist/*",
    "global.d.ts",
    "build.d.ts",
    "helpers.d.ts",
    "contracts.d.ts",
    "facades.d.ts",
    "console.d.ts",
    "stub/*"
  ],
  "scripts": {
    "build": "node build-fixer.js build && node build-fixer.js fix",
    "dev": "tsup --watch",
    "lint": "prettier . --check && eslint .",
    "prettier": "prettier . --write",
    "format": "tsc --noEmit && npm run prettier && npm run lint",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodepandai/lunox.git"
  },
  "keywords": [
    "laravel",
    "nodejs",
    "lunox",
    "lunoxjs",
    "polka",
    "svelte",
    "mvc"
  ],
  "author": {
    "name": "Akhmad Salafudin",
    "email": "axmad386@gmail.com",
    "url": "https://github.com/axmad386"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kodepandai/lunox/issues"
  },
  "homepage": "https://github.com/kodepandai/lunox#readme",
  "dependencies": {
    "body-parser": "^1.20.2",
    "colorette": "^2.0.20",
    "commander": "^11.1.0",
    "cookie": "^0.6.0",
    "dotenv": "^16.3.1",
    "formidable": "^3.5.1",
    "hash-equals": "^0.0.5",
    "pluralize": "^8.0.0",
    "polka": "1.0.0-next.23",
    "sirv": "^2.0.3"
  },
  "devDependencies": {
    "@microsoft/api-extractor": "^7.47.9",
    "@types/bcryptjs": "^2.4.6",
    "@types/body-parser": "^1.19.5",
    "@types/cookie": "^0.5.4",
    "@types/cors": "^2.8.17",
    "@types/formidable": "^3.4.5",
    "@types/node": "^20.10.0",
    "@types/pluralize": "^0.0.33",
    "@types/polka": "^0.5.7",
    "@types/react": "^18.2.39",
    "@types/react-dom": "^18.2.17",
    "@types/react-helmet": "^6.1.9",
    "@types/supertest": "^2.0.16",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "eslint": "^8.54.0",
    "glob": "^10.3.10",
    "mysql": "^2.18.1",
    "prettier": "^3.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-helmet": "^6.1.0",
    "sqlite3": "^5.1.6",
    "supertest": "^6.3.3",
    "svelte": "^4.2.7",
    "trouter": "^3.2.1",
    "tslib": "^2.6.2",
    "typescript": "^5.3.2",
    "vite": "^5.2.7",
    "vitest": "^0.34.6"
  },
  "type": "module"
}
