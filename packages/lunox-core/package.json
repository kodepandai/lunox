{
  "name": "@lunoxjs/core",
  "version": "2.0.0-next.29",
  "description": "Laravel-Flavoured NodeJs framework",
  "bin": {
    "lunox": "./bin/lunox.cjs"
  },
  "main": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.mjs",
    "./build": "./dist/build/index.mjs",
    "./helpers": "./dist/helpers.mjs",
    "./contracts": "./dist/Contracts/index.d.ts",
    "./facades": "./dist/Support/Facades/index.mjs",
    "./console": "./dist/Console/index.mjs"
  },
  "files": [
    "dist/*",
    "global.d.ts",
    "build.d.ts",
    "helpers.d.ts",
    "contracts.d.ts",
    "facades.d.ts",
    "console.d.ts",
    "stub/*"
  ],
  "scripts": {
    "build": "npm run clean && node build-fixer.mjs build && npm run fix",
    "fix": "node build-fixer.mjs fix",
    "clean": "node build-fixer.mjs clean",
    "dev": "rollup -cw & nodemon -q -w dist dist/index.js",
    "lint": "prettier . --check && eslint .",
    "prettier": "prettier . --write",
    "format": "tsc && npm run prettier && npm run lint",
    "test": "vitest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kodepandai/lunox.git"
  },
  "keywords": [
    "laravel",
    "nodejs",
    "lunox",
    "lunoxjs",
    "polka",
    "svelte",
    "mvc"
  ],
  "author": {
    "name": "Akhmad Salafudin",
    "email": "axmad386@gmail.com",
    "url": "https://github.com/axmad386"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/kodepandai/lunox/issues"
  },
  "homepage": "https://github.com/kodepandai/lunox#readme",
  "dependencies": {
    "colorette": "^2.0.16",
    "commander": "^9.2.0",
    "cookie": "^0.4.1",
    "dotenv": "^10.0.0",
    "formidable": "^2.0.1",
    "hash-equals": "^0.0.5",
    "pluralize": "^8.0.0",
    "polka": "^1.0.0-next.22",
    "sirv": "^2.0.3"
  },
  "devDependencies": {
    "@lunoxjs/build": "2.0.0-next.29",
    "@lunoxjs/test": "2.0.0-next.29",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie": "^0.4.1",
    "@types/cors": "^2.8.12",
    "@types/formidable": "^2.0.0",
    "@types/node": "^16.11.7",
    "@types/pluralize": "^0.0.29",
    "@types/polka": "^0.5.3",
    "@types/react": "^18.0.9",
    "@types/react-dom": "^18.0.4",
    "@types/react-helmet": "^6.1.5",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^6.1.0",
    "@typescript-eslint/parser": "^6.1.0",
    "bcryptjs": "^2.4.3",
    "eslint": "^8.45.0",
    "glob": "^9.2.1",
    "mysql": "^2.18.1",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.0",
    "react": "^18.1.0",
    "react-dom": "^18.1.0",
    "react-helmet": "^6.1.0",
    "rollup": "^3.26.3",
    "sqlite3": "^5.0.3",
    "supertest": "^6.3.3",
    "svelte": "^3.46.6",
    "trouter": "^3.2.1",
    "tslib": "^2.3.1",
    "typescript": "^5.1.6",
    "vite": "^4.4.6",
    "vitest": "^0.34.1"
  },
  "type": "module"
}
